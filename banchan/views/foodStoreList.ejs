<!DOCTYPE html>
<html>
    <head>
    <% include ./layout %>
    </head>
    <body>
    <% include ./topNav %>
        <div class="columnFifteen">
            <fieldset>
                <a href="">제품확인</a>
            </fieldset>
        </div>
        <div class="columnSeventyFive">
            <p>제품수 : <%= stores.count %></p>
            <table class="table_list">
                <thead>
                    <tr>
                        <th scope="cols">img</th>
                        <th scope="cols">title</th>
                        <th scope="cols">author</th>
                        <th scope="cols">timestamp</th>
                    </tr>
                </thead>
                <tbody>
                    <% stores.rows.forEach(store => { %>
                        <tr>
                            <td><img src="/img/<%= store.img %>" alt="<%= store.img %>"></td>
                            <th scope="row"><a href="/store/<%= store.url_key %>"><%= store.name %></a></td>
                            <td><%= store.manager.nickname %></td>
                            <td><%= store.createdAt %></td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
            <div class="pagination">
                <table id=pageTable>
                    <td><a href="#">&laquo;</a></td>
                    <td><a href="?page=1">1</a></td>
                    <td><a class="active" href="?page=2">2</a></td>
                    <td><a href="#">3</a></td>
                    <td><a href="#">4</a></td>
                    <td><a href="#">5</a></td>
                    <td><a href="#">6</a></td>
                    <td><a href="#">&raquo;</a></td>
                </table>
            </div>
        </div>
        <div class="columnTen">
        </div>
        <script>
        </script>
    </body>
</html>
